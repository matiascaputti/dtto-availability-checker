{
  "functions": {
    "api/check-availability.js": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/check-availability",
      "schedule": "* * * * *"
    }
  ],
  "env": {
    "TELEGRAM_BOT_TOKEN": "@telegram_bot_token",
    "TELEGRAM_CHAT_ID": "@telegram_chat_id",
    "START_TIME": "@start_time",
    "END_TIME": "@end_time"
  }
}

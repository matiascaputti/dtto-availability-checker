{
  "name": "court-availability-checker",
  "version": "1.0.0",
  "description": "Node.js app to check court availability and send Telegram notifications",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --no-daemon",
    "start:daemon": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop dtto-court-monitor",
    "restart": "pm2 restart dtto-court-monitor",
    "delete": "pm2 delete dtto-court-monitor",
    "logs": "pm2 logs dtto-court-monitor",
    "status": "pm2 status",
    "dev": "nodemon index.js",
    "monitor": "node index.js"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "node-telegram-bot-api": "^0.64.0",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "court",
    "availability",
    "telegram",
    "notifications"
  ],
  "author": "",
  "license": "MIT"
}
